<template name="15178006">
  <view class="_div data-v-7da7af4c sign_in_wrap">
    <view class="_view data-v-7da7af4c"><map id="map" longitude="{{longitude}}" latitude="{{latitude}}" scale="18" show-location style="width: 100%; height: 300px;" class="_map data-v-7da7af4c"></map></view>
    <view class="_view data-v-7da7af4c sign_in_view">
      <view class="_p data-v-7da7af4c p1">当前位置</view>
      <view class="_p data-v-7da7af4c p2">{{address}}  ></view>
    </view>
    <view class="_view data-v-7da7af4c sign_in_photo" bindtap="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}">
      <image src="../../../static/images/photograph.png" class="_image data-v-7da7af4c sign_in_img"></image>
    </view>
    <image wx:if="{{photoImg}}" mode="aspectFit" src="{{photoImg}}" class="_image data-v-7da7af4c fullwidth_img"></image>
    <view class="_view data-v-7da7af4c detail_module"></view>
    <i-button bindtap="handleProxy" type="ghost" long="{{true}}" class="_i-button data-v-7da7af4c bottom_btn" data-eventid="{{'1'}}" data-comkey="{{$k}}">确定</i-button>
  </view>
</template>