<template name="18727828">
  <view class="_div data-v-f3b2a5e4 out_work_detail_wrap">
    <i-cell title="{{outworkData.visitTheme}}" class="_i-cell data-v-f3b2a5e4">
      <view class="_p data-v-f3b2a5e4 cell_info">拜访公司：  {{outworkData.customerName}}</view>
      <view class="_p data-v-f3b2a5e4 cell_info">拜访对象：  {{outworkData.contactsName}}   |   手机号码：  {{outworkData.phone || '无'}}</view>
      <view class="_p data-v-f3b2a5e4 cell_info">拜访目的：  {{outworkData.visitObjective}}</view>
      <view class="_view data-v-f3b2a5e4 btn_group" wx:if="{{authority}}">
        <i-button class="_i-button data-v-f3b2a5e4 flex_btn" type="subject" size="small" bindtap="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}">通过</i-button>
        <i-button class="_i-button data-v-f3b2a5e4 flex_btn" type="ghost" size="small" bindtap="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}">拒绝</i-button>
      </view>
      <i-cell title="{{'审核状态：' + outworkData.auditStatus}}" i-class="cell_link" class="_i-cell data-v-f3b2a5e4"></i-cell>
    </i-cell>
    <i-panel title=" " class="_i-panel data-v-f3b2a5e4">
      <i-tabs current="{{activeName}}" bindchange="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}" class="_i-tabs data-v-f3b2a5e4">
        <i-tab key="first" title="审核历史" class="_i-tab data-v-f3b2a5e4"></i-tab>
        <i-tab key="second" title="基本信息" class="_i-tab data-v-f3b2a5e4"></i-tab>
        <i-tab key="third" title="相关信息" class="_i-tab data-v-f3b2a5e4"></i-tab>
      </i-tabs>
    </i-panel>
    <view wx:if="{{activeName == 'first'}}" class="_view data-v-f3b2a5e4 white_bg font_size_12">
      <view class="_view data-v-f3b2a5e4 detail_module"></view>
      <i-steps current="{{stepIndex}}" direction="vertical" i-class="stage_steps" class="_i-steps data-v-f3b2a5e4">
        <i-step wx:key="item.orderId" key="{{item.orderId}}" status="{{item.stepStatus}}" icon="{{item.stepIcon}}" wx:for="{{auditLog}}" wx:for-index="index" wx:for-item="item" class="_i-step data-v-f3b2a5e4">
          <i-fiche isContent isFooter slot="title" title="{{item.realname}}" extra="{{item.auditState}}" thumb="{{item.portrait}}" class="_i-fiche data-v-f3b2a5e4">
            <view slot="content" class="_view data-v-f3b2a5e4">{{item.remarks}}</view>
            <view slot="footer" class="_view data-v-f3b2a5e4">{{item.examineTime}}</view>
          </i-fiche>
        </i-step>
      </i-steps>
    </view>
    <view wx:if="{{activeName == 'second'}}" class="_view data-v-f3b2a5e4 font_size_12">
      <i-cell-group class="_i-cell-group data-v-f3b2a5e4">
        <i-cell title="拜访时间" value="{{outworkData.visitTime}}" class="_i-cell data-v-f3b2a5e4"></i-cell>
        <i-cell title="结束时间" value="{{outworkData.endTime}}" class="_i-cell data-v-f3b2a5e4"></i-cell>
        <i-cell title="提醒时间" value="{{outworkData.remindTime}}" class="_i-cell data-v-f3b2a5e4"></i-cell>
        <i-cell title="协助人员" class="_i-cell data-v-f3b2a5e4"><label slot="footer" wx:key="item.private_id" key="{{item.private_id}}" wx:for="{{outworkData.privateUser}}" wx:for-index="index" wx:for-item="item" class="_span data-v-f3b2a5e4"><label wx:if="{{index !== 0}}" class="_span data-v-f3b2a5e4">，</label>
            {{item.private_name}}
          </label></i-cell>
        <i-cell title="状态" value="{{outworkData.state}}" class="_i-cell data-v-f3b2a5e4"></i-cell>
        <i-cell title="负责人" value="{{outworkData.private_employee}}" class="_i-cell data-v-f3b2a5e4"></i-cell>
        <i-cell title="部门" value="{{outworkData.deptname}}" class="_i-cell data-v-f3b2a5e4"></i-cell>
        <i-cell title="机构" value="{{outworkData.parentname}}" class="_i-cell data-v-f3b2a5e4"></i-cell>
      </i-cell-group>
    </view>
    <view wx:if="{{activeName == 'third'}}" class="_view data-v-f3b2a5e4 font_size_12">
      <i-card full isContent title="执行结果" extra="{{outworkData.state}}" class="_i-card data-v-f3b2a5e4">
        <view slot="content" class="_view data-v-f3b2a5e4">
          <view class="_p data-v-f3b2a5e4">签到时间：{{outworkData.timeCheck || ''}}</view>
          <view class="_p data-v-f3b2a5e4">签到地址：{{outworkData.addressCheck || ''}}</view>
          <view class="_br data-v-f3b2a5e4"></view>
          <image wx:if="{{outworkData.photoCheck}}" mode="scaleToFill" src="{{outworkData.photoCheck}}" class="_image data-v-f3b2a5e4 follow_image"></image>
        </view>
      </i-card>
    </view>
    <i-tab-bar current="{{activeBar}}" bindchange="handleProxy" class="_i-tab-bar data-v-f3b2a5e4 bottom_btn" data-eventid="{{'3'}}" data-comkey="{{$k}}">
      <i-tab-bar-item key="coordinates" icon="coordinates" current-icon="coordinates" title="签到" class="_i-tab-bar-item data-v-f3b2a5e4"></i-tab-bar-item>
      <i-tab-bar-item key="collection" icon="collection" current-icon="collection" title="打分" class="_i-tab-bar-item data-v-f3b2a5e4"></i-tab-bar-item>
      <i-tab-bar-item key="more" icon="more" current-icon="more" title="更多" class="_i-tab-bar-item data-v-f3b2a5e4"></i-tab-bar-item>
    </i-tab-bar>
    <i-action-sheet visible="{{showMore}}" actions="{{moreActions}}" show-cancel bindcancel="handleProxy" bindchange="handleProxy" mask-closable="{{false}}" data-eventid="{{'4'}}" data-comkey="{{$k}}" class="_i-action-sheet data-v-f3b2a5e4"></i-action-sheet>
    <i-toast id="toast" class="_i-toast data-v-f3b2a5e4"></i-toast>
    <i-message id="message" class="_i-message data-v-f3b2a5e4"></i-message>
  </view>
</template>