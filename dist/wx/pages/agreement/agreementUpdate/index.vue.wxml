<template name="f3bce298">
  <view class="_div data-v-ef40bcc8 add_or_update_wrap">
    <i-panel title=" " class="_i-panel data-v-ef40bcc8"></i-panel>
    <i-panel title="{{agreementName}}" class="_i-panel data-v-ef40bcc8">
      <i-cell title="合同类型" value="{{updateList.contract_type}}" request is-link i-class="simple_cell" bindtap="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}" class="_i-cell data-v-ef40bcc8"></i-cell>
      <i-input value="{{updateList.contract_name}}" bindchange="handleProxy" title="合同名称" right request maxlength="20" bindinput="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}" class="_i-input data-v-ef40bcc8"></i-input>
      <i-cell title="客户名称" value="{{updateList.customerpoolName}}" request is-link i-class="simple_cell" bindtap="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}" class="_i-cell data-v-ef40bcc8"></i-cell>
      <i-cell title="对应商机" value="{{updateList.opportunityName}}" is-link i-class="simple_cell" bindtap="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}" class="_i-cell data-v-ef40bcc8"></i-cell>
      <i-input value="{{updateList.amount}}" bindchange="handleProxy" title="合同金额" right request type="number" maxlength="50" bindinput="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}" class="_i-input data-v-ef40bcc8"></i-input>
      <i-cell title="开始时间" request is-link i-class="simple_cell" class="_i-cell data-v-ef40bcc8">
        <picker slot="footer" mode="date" value="{{updateList.start_date}}" start="{{nowDate}}" bindchange="handleProxy" data-eventid="{{'5'}}" data-comkey="{{$k}}" class="_picker data-v-ef40bcc8">
          <view class="_view data-v-ef40bcc8 picker cell_picker">
            {{updateList.start_date}}
          </view>
        </picker>
      </i-cell>
      <i-cell title="结束时间" request is-link i-class="simple_cell" class="_i-cell data-v-ef40bcc8">
        <picker slot="footer" mode="date" value="{{updateList.end_date}}" start="{{nowDate}}" bindchange="handleProxy" data-eventid="{{'6'}}" data-comkey="{{$k}}" class="_picker data-v-ef40bcc8">
          <view class="_view data-v-ef40bcc8 picker cell_picker">
            {{updateList.end_date}}
          </view>
        </picker>
      </i-cell>
      <i-cell title="客户签约人" value="{{updateList.signatorieName}}" request is-link i-class="simple_cell" bindtap="handleProxy" data-eventid="{{'7'}}" data-comkey="{{$k}}" class="_i-cell data-v-ef40bcc8"></i-cell>
      <i-input value="{{updateList.our_signatorieName}}" bindchange="handleProxy" title="我方签约人" right maxlength="50" disabled data-eventid="{{'8'}}" data-comkey="{{$k}}" class="_i-input data-v-ef40bcc8"></i-input>
      <i-input value="{{updateList.remarks}}" bindchange="handleProxy" title="备注" right type="textarea" maxlength="200" bindinput="handleProxy" data-eventid="{{'9'}}" data-comkey="{{$k}}" class="_i-input data-v-ef40bcc8"></i-input>
    </i-panel>
    <view class="_p data-v-ef40bcc8 request_tip"><label style="color:#f56c6c" class="_span data-v-ef40bcc8"> * </label>为必填项</view>
    <i-button bindtap="handleProxy" type="ghost" long="{{true}}" class="_i-button data-v-ef40bcc8 bottom_btn" data-eventid="{{'10'}}" data-comkey="{{$k}}">确定</i-button>
    <i-action-sheet visible="{{showType}}" actions="{{typeList}}" bindchange="handleProxy" data-eventid="{{'11'}}" data-comkey="{{$k}}" class="_i-action-sheet data-v-ef40bcc8"></i-action-sheet>
    <i-action-sheet visible="{{showOppo}}" actions="{{oppoList}}" show-cancel bindcancel="handleProxy" bindchange="handleProxy" data-eventid="{{'12'}}" data-comkey="{{$k}}" class="_i-action-sheet data-v-ef40bcc8"></i-action-sheet>
    <i-action-sheet visible="{{showsignator}}" actions="{{signatorList}}" bindchange="handleProxy" data-eventid="{{'13'}}" data-comkey="{{$k}}" class="_i-action-sheet data-v-ef40bcc8"></i-action-sheet>
    <i-message id="message" class="_i-message data-v-ef40bcc8"></i-message>
    <i-toast id="toast" class="_i-toast data-v-ef40bcc8"></i-toast>
  </view>
</template>