<template name="9c52edca">
  <view class="_div data-v-22785fcc note_wrap">
    <view class="_view data-v-22785fcc search_view">
      <i-icon type="other" size="18" color="#80848f" class="_i-icon data-v-22785fcc search_icon" bindtap="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}"></i-icon>
    </view>
    <i-drawer mode="right" visible="{{searchCriteria}}" bindclose="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}" class="_i-drawer data-v-22785fcc">
      <view class="_view data-v-22785fcc search_container">
        <i-panel title="个人便签" i-class="query_label" class="_i-panel data-v-22785fcc">
          <view class="_view data-v-22785fcc query_view"><label class="_span data-v-22785fcc queryBtn {{[index == typeActive ? 'isActive' : '']}}" wx:key="item.id" key="{{item.id}}" bindtap="handleProxy" data-eventid="{{'1_'+index}}" data-comkey="{{$k}}" wx:for="{{typeList}}" wx:for-index="index" wx:for-item="item">{{item.name}}</label></view>
        </i-panel>
        <i-button bindtap="handleProxy" type="ghost" size="small" long="true" class="_i-button data-v-22785fcc reset_btn" data-eventid="{{'2'}}" data-comkey="{{$k}}">重置</i-button>
      </view>
    </i-drawer>
    <i-swipeout i-class="i-swipeout-demo-item" operateWidth="{{60}}" wx:key="item.id" key="{{item.id}}" wx:for="{{tableData}}" wx:for-index="index" wx:for-item="item" class="_i-swipeout data-v-22785fcc">
      <view slot="content" class="_view data-v-22785fcc">
        <i-cell i-class="cell_content" class="_i-cell data-v-22785fcc">
          <rich-text nodes="{{item.name}}" class="_rich-text data-v-22785fcc"></rich-text>
          <view class="_view data-v-22785fcc cell_footer"> 创建时间：{{item.opportunity_deal}}
          </view>
        </i-cell>
      </view>
      <view slot="button" class="_view data-v-22785fcc i-swipeout-button">
        <view class="_view data-v-22785fcc i-swipeout-button-item" style="width:60px;background-color:#f56c6c" bindtap="handleProxy" data-eventid="{{'4_'+index}}" data-comkey="{{$k}}">
          <i-icon size="24" type="trash" style="color:#fff" class="_i-icon data-v-22785fcc"></i-icon>
        </view>
      </view>
    </i-swipeout>
    <picker mode="multiSelector" bindchange="handleProxy" bindcolumnchange="handleProxy" value="{{multiIndex}}" range="{{multiArray}}" data-eventid="{{'5'}}" data-comkey="{{$k}}" class="_picker data-v-22785fcc">
      <view class="_view data-v-22785fcc picker">
        <i-button type="ghost" long="{{true}}" class="_i-button data-v-22785fcc bottom_btn">新增</i-button>
      </view>
    </picker>
  </view>
</template>