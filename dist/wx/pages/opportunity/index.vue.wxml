<template name="150eabb4">
  <view class="_div data-v-0c6f7a01 opportunity_wrap">
    <view class="_view data-v-0c6f7a01 search_view">
      <view class="_view data-v-0c6f7a01 search_box">
        <i-icon type="search" size="16" color="#80848f" class="_i-icon data-v-0c6f7a01 search_icon"></i-icon>
        <i-input value="{{searchList.searchName}}" bindchange="handleProxy" maxlength="50" i-class="search_input" bindinput="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}" class="_i-input data-v-0c6f7a01"></i-input>
        <i-icon wx:if="{{isValue}}" type="close" size="14" color="#80848f" class="_i-icon data-v-0c6f7a01 search_icon" bindtap="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}"></i-icon>
      </view> <label class="_span data-v-0c6f7a01 search_btn" bindtap="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}">搜索</label>
      <i-icon type="other" size="18" color="#80848f" class="_i-icon data-v-0c6f7a01 search_icon" bindtap="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}"></i-icon>
    </view>
    <i-drawer mode="right" visible="{{searchCriteria}}" bindclose="handleProxy" data-eventid="{{'9'}}" data-comkey="{{$k}}" class="_i-drawer data-v-0c6f7a01">
      <view class="_view data-v-0c6f7a01 search_container">
        <i-panel title="数据授权" i-class="query_label" class="_i-panel data-v-0c6f7a01">
          <view class="_view data-v-0c6f7a01 query_view"><label class="_span data-v-0c6f7a01 queryBtn {{[index == powerActive ? 'isActive' : '']}}" wx:key="item.label" key="{{item.label}}" bindtap="handleProxy" data-eventid="{{'4_'+index}}" data-comkey="{{$k}}" wx:for="{{powerList}}" wx:for-index="index" wx:for-item="item">{{item.name}}</label></view>
        </i-panel>
        <i-panel title="商机进度" i-class="query_label" class="_i-panel data-v-0c6f7a01">
          <view class="_view data-v-0c6f7a01 query_view"><label class="_span data-v-0c6f7a01 queryBtn {{[index == stateActive ? 'isActive' : '']}}" wx:key="item.step_id" key="{{item.step_id}}" bindtap="handleProxy" data-eventid="{{'5_'+index}}" data-comkey="{{$k}}" wx:for="{{stateList}}" wx:for-index="index" wx:for-item="item">{{item.step_name}}</label></view>
        </i-panel>
        <i-panel title="商机预测" i-class="query_label" class="_i-panel data-v-0c6f7a01">
          <view class="_view data-v-0c6f7a01 query_view"><label class="_span data-v-0c6f7a01 queryBtn {{[index == predActive ? 'isActive' : '']}}" wx:key="item.label" key="{{item.label}}" bindtap="handleProxy" data-eventid="{{'6_'+index}}" data-comkey="{{$k}}" wx:for="{{predList}}" wx:for-index="index" wx:for-item="item">{{item.name}}</label></view>
        </i-panel>
        <i-panel title="新增时间" i-class="query_label" class="_i-panel data-v-0c6f7a01">
          <view class="_view data-v-0c6f7a01 query_view"><label class="_span data-v-0c6f7a01 queryBtn {{[index == timeActive ? 'isActive' : '']}}" wx:key="item.label" key="{{item.label}}" bindtap="handleProxy" data-eventid="{{'7_'+index}}" data-comkey="{{$k}}" wx:for="{{timeList}}" wx:for-index="index" wx:for-item="item">{{item.name}}</label></view>
        </i-panel>
        <i-button bindtap="handleProxy" type="ghost" size="small" long="true" class="_i-button data-v-0c6f7a01 reset_btn" data-eventid="{{'8'}}" data-comkey="{{$k}}">重置</i-button>
      </view>
    </i-drawer>
    <i-swipeout i-class="i-swipeout-demo-item" operateWidth="{{60}}" wx:key="item.id" key="{{item.id}}" wx:for="{{tableData}}" wx:for-index="index" wx:for-item="item" class="_i-swipeout data-v-0c6f7a01">
      <view slot="content" bindtap="handleProxy" data-eventid="{{'10_'+index}}" data-comkey="{{$k}}" class="_view data-v-0c6f7a01">
        <i-cell i-class="cell_content" title="{{item.opportunity_name}}" label="{{item.customerpool[0].name}}" class="_i-cell data-v-0c6f7a01">
          <wxs module="tools" src="./../utils/comming.wxs" class="_wxs data-v-0c6f7a01"></wxs>
          <view class="_view data-v-0c6f7a01 cell_footer"> 负责人：{{item.private_employee}}   |   预计成交金额： {{item.opportunity_achievement}}
          </view>
          <view class="_view data-v-0c6f7a01 cell_footer"> 预计成交时间：{{item.opportunity_deal}}   |   阶段：{{item.opportunityProgress[0].progress_name}}
          </view>
        </i-cell>
      </view>
      <view slot="button" class="_view data-v-0c6f7a01 i-swipeout-demo-button-group">
        <view class="_view data-v-0c6f7a01 i-swipeout-demo-button" style="width:60px;background-color:#f5f5f5" bindtap="handleProxy" data-eventid="{{'11_'+index}}" data-comkey="{{$k}}">
          <i-icon size="24" type="editor" style="line-height:114px;margin-left:18px;color:#80848f" class="_i-icon data-v-0c6f7a01"></i-icon>
        </view>
      </view>
    </i-swipeout>
    <i-load-more wx:if="{{noMore}}" tip="我是有底线的" loading="{{false}}" class="_i-load-more data-v-0c6f7a01"></i-load-more>
    <i-button bindtap="handleProxy" type="ghost" long="{{true}}" class="_i-button data-v-0c6f7a01 bottom_btn" data-eventid="{{'12'}}" data-comkey="{{$k}}">新增</i-button>
  </view>
</template>