@import "./common/vendor.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.search_view {
  width: 100%;
  height: 80rpx;
  background-color: #f5f5f5;
  padding: 0 20rpx;
  box-sizing: border-box;
  position: fixed;
  top: 0;
  display: flex;
  align-items: center;
  z-index: 99;
}
.search_box {
  height: 60rpx;
  line-height: 60rpx;
  background-color: #ffffff;
  border: 2rpx solid #f5f5f5;
  border-radius: 10rpx;
  padding: 6rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.search_icon {
  display: inherit;
  border-radius: 10rpx;
  flex: 0;
}
.search_input {
  flex: 1;
  width: 480rpx;
  height: 56rpx;
  line-height: 50rpx;
  padding: 0 16rpx !important;
  box-sizing: border-box;
}
.search_btn {
  font-size: 28rpx;
  color: #62a072;
  line-height: 60rpx;
  margin: 0 20rpx;
}
.search_container {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
  font-size: 24rpx;
}
.query_view {
  padding: 10rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  position: relative;
}
.query_label {
  font-size: 26rpx;
}
.queryBtn {
  margin: 8rpx 16rpx !important;
  font-size: 24rpx !important;
  width: 120rpx;
  padding: 8rpx 0;
  text-align: center;
}
.isActive {
  color: #ff5722;
  border-bottom: 1rpx solid #ff5722;
}
.reset_btn {
  position: absolute;
  bottom: 0;
  width: 100%;
  font-size: 26rpx;
}
.cell_content {
  padding: 0 !important;
  font-size: 28rpx;
}
.cell_footer {
  font-size: 24rpx;
  color: #acacac;
  padding-top: 10rpx;
  box-sizing: border-box;
}
.cell_label {
  color: #80848f;
  font-size: 24rpx;
}
.pool_cell {
  padding: 30rpx 40rpx !important;
}
.i-swipeout-button {
  height: 100%;
  display: flex;
}
.i-swipeout-button-item {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.margin_card {
  margin: 20rpx 20rpx 0 20rpx !important;
}
.contact_span1 {
  color: #80848f;
}
.contact_span2 {
  color: #919191;
}
.contact_way {
  display: inline-block;
  width: 80rpx;
  height: 36rpx;
  line-height: 36rpx;
  background-color: #80848faf;
  color: #ffffff;
  border-radius: 18rpx;
  text-align: center;
}
.bottom_List {
  margin: 10rpx 30rpx;
  position: fixed;
  bottom: 0;
}
.nomargin_btn {
  margin: 0 !important;
}
.fullwidth_img {
  margin-top: 20rpx;
  width: 100%;
}
.follow_image {
  margin-top: 20rpx;
  width: 140rpx;
  height: 140rpx;
}
.bottom_btn {
  position: fixed;
  bottom: 0;
  width: 100%;
  z-index: 9;
}
.bottom_view {
  width: 100vw;
  height: 80rpx;
  background-color: #fff;
  border-top: 1rpx solid #f5f5f5;
  font-size: 28rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: fixed;
  bottom: 0;
  z-index: 9;
}
.followimg_btn {
  margin-top: 60rpx !important;
}
.btn_group {
  width: 100%;
  margin-top: 16rpx;
  display: flex;
}
.flex_btn {
  flex: 1;
}
.request_tip {
  margin: 60rpx 0 0 30rpx;
  font-size: 24rpx;
  color: #80848f;
}
.font_size_12 {
  font-size: 24rpx;
}
.gray_12 {
  font-size: 24rpx;
  color: #80848f;
}
.simple_cell {
  position: relative;
  padding: 20rpx 30rpx !important;
}
.simple_cell::after {
  display: initial !important;
}
.color_495060_text {
  color: #495060;
}
.cell_picker {
  min-width: 200rpx;
  height: 48rpx;
}
.vice_panel {
  font-size: 24rpx;
  color: #80848f;
}
.small_panel {
  font-size: 28rpx;
}
.bgc_span {
  display: inline-block;
  padding: 4rpx 16rpx;
  box-sizing: border-box;
  background-color: rgb(241, 250, 255);
  border-radius: 16rpx;
}
.detail_module {
  width: 100%;
  height: 20rpx;
}
.cell_info {
  font-size: 24rpx;
  color: #80848f;
  padding-top: 12rpx;
  box-sizing: border-box;
}
.cell_link {
  background-color: rgb(241, 250, 255) !important;
  width: 100%;
  margin-top: 16rpx;
  padding: 10rpx !important;
  color: #5f6164;
}
.cell_link .i-cell-text {
  font-size: 24rpx !important;
}
.add_or_update_wrap {
  margin-bottom: 80rpx;
  padding-bottom: 20rpx;
}
.white_bg {
  background-color: #fff;
}
.note_input {
  min-height: 600rpx !important;
}
.order_search {
  position: fixed;
  top: 0;
  width: 100%;
  background-color: #fafafa;
  z-index: 10;
}
.order_searck_box {
  width: calc(100% - 60rpx);
  background-color: #fff;
  margin: 20rpx 30rpx;
  border: 1rpx solid #fafafa;
  border-radius: 8rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
}
.order_search_input {
  flex: 0 0 90%;
  font-size: 28rpx;
  height: 56rpx;
  line-height: 56rpx;
  padding: 4rpx 10rpx;
  box-sizing: border-box;
}
.order_tabs {
  height: 72rpx !important;
  line-height: 72rpx !important;
}
.bar_option {
  width: 100vw;
  background-color: rgb(255, 255, 255);
  text-align: center;
  position: fixed;
  z-index: 999;
}
.option_p {
  font-size: 26rpx;
  line-height: 72rpx;
  border-bottom: 1rpx solid #f5f5f5;
}
.theme_color_text {
  color: #ff6333;
}
.gray_color_text {
  color: #acacac;
}
.product_wrap {
  flex: 3;
  height: calc(100vh -160rpx);
  background-color: rgb(255, 255, 255);
  float: left;
}
.product_item {
  padding: 16rpx;
  font-size: 28rpx;
  border-bottom: 1rpx solid #f5f5f5;
}
.product_item_c {
  position: relative;
}
.product_item_price {
  position: absolute;
  top: 4rpx;
  left: 160rpx;
}
.product_item_amount {
  position: absolute;
  bottom: 4rpx;
  left: 160rpx;
}
.product_item_counter {
  position: absolute;
  bottom: 0;
  right: 4rpx;
  height: 52rpx;
  line-height: 52rpx;
  border-radius: 24rpx;
  background-color: #f5f5f5;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.counter_btn {
  display: inline-block;
  width: 52rpx;
  text-align: center;
}
.counter_disable {
  color: #a8a8a8;
}
.counter_text {
  display: inline-block;
  width: 84rpx;
  text-align: center;
  font-size: 28rpx;
}
.product_item_shopping {
  position: absolute;
  bottom: 0;
  right: 40rpx;
  height: 56rpx;
  width: 56rpx;
  border-radius: 26rpx;
  background-color: #e62c2c;
  display: flex;
  align-items: center;
  justify-content: center;
}
.product_item_add {
  position: absolute;
  bottom: 0;
  right: 40rpx;
  height: 56rpx;
  width: 56rpx;
  border-radius: 26rpx;
  background-color: #ff6333;
  display: flex;
  align-items: center;
  justify-content: center;
}
.product_item_update {
  position: absolute;
  right: 40rpx;
  top: 4rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.product_item_more {
  position: fixed;
  top: 0;
  z-index: 98;
}
.item_more_head {
  width: 100vw;
  height: 200rpx;
  background-color: rgba(0, 0, 0, 0.281);
}
.item_more_content {
  height: calc(100vh - 200rpx);
  background-color: #fff;
  position: relative;
}
.item_more_close {
  color: #c7c7c7;
  position: absolute;
  right: 20rpx;
  top: 40rpx;
}
.item_more_goods {
  padding: 40rpx 20rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  display: flex;
}
.item_more_scroll_wrap {
  height: calc(100vh - 480rpx);
}
.item_more_goods_item {
  height: 160rpx;
  position: relative;
  padding: 40rpx 20rpx;
  font-size: 28rpx;
  border-bottom: 1rpx solid #e9eaec;
}
.goods_item_head {
  display: flex;
  align-items: center;
}
.goods_item_foot {
  position: absolute;
  bottom: 40rpx;
}
.product_counter {
  background-color: rgba(0, 0, 0, 0.281);
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  z-index: 99;
}
.counter_wrap {
  position: relative;
  background-color: #fff;
  width: 80%;
  margin-left: 10%;
  margin-top: 50%;
}
.counter_head {
  background-color: #ff6333;
  padding: 20rpx;
  font-size: 14;
  color: #fff;
}
.counter_content {
  position: relative;
}
.counter_item {
  height: 52rpx;
  line-height: 52rpx;
  border-radius: 24rpx;
  background-color: #f5f5f5;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.counter_foot {
  padding: 10rpx;
  display: flex;
  text-align: center;
  justify-content: center;
}
.counter_foot ._span {
  flex: 1;
  line-height: 56rpx;
}
.inline_btn {
  display: inline-block;
  margin-right: 0 !important;
  width: 160rpx;
}